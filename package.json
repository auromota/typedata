{
  "name": "typedata",
  "version": "0.0.6",
  "description": "A bundle of data collections in TypeScript",
  "main": "dist/TypeData.js",
  "scripts": {
    "prepublish": "tsc && dts-generator --name typedata --project . --out index.d.ts",
    "test": "mocha --compilers ts:ts-node/register --opts mocha.opts",
    "test:auto": "mocha --compilers ts:ts-node/register --opts mocha.opts --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/auromota/typedata.git"
  },
  "author": "Auro Mota",
  "license": "MIT",
  "typescript": {
    "definition": "index.d.ts"
  },
  "bugs": {
    "url": "https://github.com/auromota/typedata/issues"
  },
  "homepage": "https://github.com/auromota/typedata#readme",
  "dependencies": {
    "@types/lodash": "^4.14.36",
    "@types/mocha": "^2.2.32",
    "dts-generator": "^1.7.0",
    "lodash": "^4.0.0"
  },
  "keywords": [
    "data",
    "structures",
    "typescript",
    "generics",
    "list",
    "iterable",
    "linked",
    "collections"
  ],
  "devDependencies": {
    "@types/chai": "^3.4.34",
    "chai": "^3.5.0",
    "mocha": "^3.1.0",
    "ts-node": "^1.3.0",
    "typescript": "^2.0.3"
  }
}